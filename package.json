{
  "name": "grpc-connect-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "clean": "rimraf node_modules dist",
    "codegen": "buf generate buf.build/connectrpc/eliza",
    "dev": "vite",
    "fix": "run-s _lint:fix _format:fix",
    "lint": "run-s _lint _format",
    "preview": "vite preview",
    "_format": "prettier --list-different '*.{js,ts,json,md}' 'src/**/*.{css,js,jsx,ts,tsx,json,md}'",
    "_format:fix": "pnpm _format -- --write",
    "_lint": "eslint .",
    "_lint:fix": "pnpm _lint -- --fix"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.2",
    "@connectrpc/connect": "^2.1.1",
    "@connectrpc/connect-web": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^6.4.1"
  }
